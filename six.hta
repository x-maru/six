<!DOCTYPE html>
<html>
<head>
<meta http-equiv="X-UA-Compatible" content="IE=9">
<hta:application id="app" NAVIGABLE="yes" />
<script type="text/javascript">
// IE9 HTA エントリ: 起動引数を解析して _six.html?open=... に引き継ぐ
(function(){
	function parseArg(cl){
		try{
			if(!cl) return null;
			cl = String(cl);
			var m = cl.match(/\.hta["']?\s+(.+)$/i);
			if(!m) return null;
			var rest = m[1].trim();
			if(!rest) return null;
			if(rest.charAt(0)==='\"'){
				var m2 = rest.match(/^"([^"]+)"/);
				if(m2 && m2[1]) return m2[1];
			}
			if(rest.charAt(0)==="'"){
				var m3 = rest.match(/^'([^']+)'/);
				if(m3 && m3[1]) return m3[1];
			}
			return rest.split(/\s+/)[0];
		}catch(_){ return null; }
	}
	function go(){
		var arg = null;
		try{
			if (window.app && (app.CommandLine || app.commandLine)){
				arg = parseArg(String(app.CommandLine || app.commandLine || ''));
			}
		}catch(_){ }
		var url = '_six.html';
		if (arg){ url += '?open=' + encodeURIComponent(arg); }
		// 履歴を汚さない
		window.location.replace(url);
	}
	if (document.readyState === 'complete') go();
	else window.onload = go;
})();
</script>
</head>
<body></body>
</html>
